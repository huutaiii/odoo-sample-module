<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="test_tag_action" model="ir.actions.act_window">
        <field name="name">Tag action</field>
        <field name="res_model">test.tag</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="test_tag_view" model="ir.ui.view">
        <field name="name">tag view</field>
        <field name="model">test.tag</field>
        <field name="arch" type="xml">
            <tree string="Channel">
                <field name="name" />
            </tree>
        </field>
    </record>
    <menuitem id="test_tag_menu_action" action="test_tag_action"/>
</odoo>